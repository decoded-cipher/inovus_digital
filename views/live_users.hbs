<div class="container-fluid container-fluid-div">
    <div class="container container-div">
        <div class="row">
            <div class="col-lg-12">

                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th scope="col">No:</th>
                            <th scope="col">Photo</th>
                            <th scope="col">Name</th>
                            <th scope="col">Program</th>
                            <th scope="col">Role</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each userData}}
                        <tr>
                            <th scope="row">1</th>
                            <td><img src="https://api.dicebear.com/7.x/initials/svg?seed={{this.name}}" alt="Image" style="width: 50px; height:auto;"></td>
                            <td>{{this.name}}</td>
                            <td>{{this.program}}</td>
                            <td>{{this.role}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>

            </div>
        </div>
    </div>
</div>  